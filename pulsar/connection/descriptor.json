{
	"name": "pulsar-connection",
	"type": "flogo:connector",
	"version": "0.1.1",
	"title": "Apache Pulsar Connection",
	"description": "A simple pulsar connection",
	"settings": [
		{
			"name": "name",
			"type": "string",
			"required": true
		},
		{
			"name": "description",
			"type": "string",
			"required": false
		},
		{
			"name": "url",
			"type": "string",
			"required": true
		},
		{
			"name": "auth",
			"type": "string",
			"required": true,
			"value": "None",
			"allowed": ["None","TLS","JWT","Athenz"]
		},
		{
			"name": "allowInsecure",
			"type": "boolean",
			"required": true,
			"value": false
		},
		{
			"name": "caCert",
			"type": "string",
			"required": false,
			"value": ""
		},
		{
			"name": "certFile",
			"type": "string"
		},
		{
			"name": "keyFile",
			"type": "string"
		},
		{
			"name": "athenzAuth",
			"type": "params"
		},
		{
			"name": "jwt",
			"type": "string",
			"required": false,
			"value": ""
		}
	]
}
